{
    "name": "kashi93/vure",
    "license": "MIT",
    "type": "library",
    "description": "Vure provides a simple starting point for building a SPA (single-page application) using Vue or React with the Laravel framework. The code structure remains unchanged, and Vure can be styled using either Bootstrap or Tailwind.",
    "keywords": [
        "laravel",
        "auth"
    ],
    "support": {
        "issues": "https://github.com/kashi93/vure/issues",
        "source": "https://github.com/kashi93/vure"
    },
    "authors": [
        {
            "name": "Kashi",
            "email": "zarkashi93@yahoo.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "laravel/sanctum": "^3.2",
        "illuminate/console": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/validation": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Kashi93\\Vure\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Kashi93\\Vure\\VureServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}